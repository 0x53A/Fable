{
    "env": "node",
    "outDir": "out",
    "sourceMaps": true,
    "projFile": "src/Fable.Samples.React.fsproj",
    "babelPlugins": ["transform-runtime", "add-module-exports"],
    "scripts": {
        "prebuild": "npm install",
        "postbuild": "node node_modules/webpack/bin/webpack"
    },
    "targets": {
        "debug": {
            "watch": true,
            "symbols": ["WEBPACK_DEV_SERVER"],
            "scripts": {
                "postbuild": "node out/server"
            }
        },
        "production": {
            "sourceMaps": false,
            "scripts": {
                "postbuild": "node node_modules/webpack/bin/webpack -p"
            }
        }
    }
}